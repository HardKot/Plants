{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2019", "DOM", /* "ES2016" */],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "~components/*": ["./src/core/components/*"],
      "~services/*": ["./src/core/services/*"],
      "~store": ["./src/core/store"],
      "~screens/*": ["./src/core/screens/*"],
      "~assets/*": ["./src/core/assets/*"],
      "~containers": ["./sr/corec/containers"],
      "~models/*": ["./src/core/models/*"],
      "~i18n": ["./src/core/i18n"],
      "~constant": ["./src/core/constant"],
      "~routes": ["./src/core/routes"],
      "~elements/*": ["./src/core/elements/*"],
      "~icons": ["./src/core/elements/icons"],
      "~layouts/*": ["./src/core/layouts/*"],
      "~globalStyle": ["./src/core/globalStyle"]
    },
    "noImplicitThis": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}