{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2019"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "~components": ["./src/components"],
      "~services/*": ["./src/services/*"],
      "~store": ["./src/store"],
      "~screens/*": ["./src/screens/*"],
      "~assets/*": ["./src/assets/*"],
      "~containers": ["./src/containers"],
      "~models/*": ["./src/models/*"],
      "~i18n": ["./src/i18n"],
      "~constant": ["./src/constant"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  
}