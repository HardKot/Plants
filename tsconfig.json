{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "~components": [
        "./src/components"
      ],
      "~services": [
        "./src/services"
      ],
      "~store": [
        "./src/store"
      ],
      "~screens": [
        "./src/screens"
      ],
      "~assetc": [
        "./src/assets"
      ],
      "~containers": [
        "./src/containers"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
